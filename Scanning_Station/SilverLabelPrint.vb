Imports System.Drawing.Printing
Imports System.IO
Imports System.Deployment.Application
Imports Library3

Public Class SilverLabelPrint
    Dim Coordinats As ArrayList
    Dim LOTID, IDApp As Integer
    Dim SN_InLOT_Ready As Integer
    Public Sub New(LOTIDWF As Integer, IDApp As Integer)
        InitializeComponent()
        Me.LOTID = LOTIDWF
        Me.IDApp = IDApp
    End Sub
#Region "Загрузка формы"
    Private Sub SilverLabelPrint_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        Dim myVersion As Version
        If ApplicationDeployment.IsNetworkDeployed Then
            myVersion = ApplicationDeployment.CurrentDeployment.CurrentVersion
        End If
        LB_SW_Wers.Text = String.Concat("v", myVersion)
        SN_InLOT_Ready = SelectInt($"select COUNT (*) from [FAS].[dbo].[CT_Aquarius]where LOTID = {LOTID} and  IsPrinted = 0")
        LB_SN_InLOT_Ready.Text = SN_InLOT_Ready
        If SN_InLOT_Ready > 0 Then
            LB_SN_InLOT_Ready.ForeColor = Color.Green
            If SN_InLOT_Ready < 500 Then
                Num_SNToPrint.Maximum = SN_InLOT_Ready
            End If
        Else
            LB_COUNT_INLOT.ForeColor = Color.Red
            LB_SN_InLOT_Ready.Text = ""
            LB_COUNT_INLOT.Text = "Номера в лоте закончились"
            Num_SNToPrint.Enabled = False
        End If
#Region "Обнаружение принтеров и установка дефолтного принтера"
        For Each item In PrinterSettings.InstalledPrinters
            If InStr(item.ToString(), "TSC TX600") Then
                CB_DefaultPrinter.Items.Add(item.ToString())
            End If
        Next
        If CB_DefaultPrinter.Items.Count <> 0 Then
            CB_DefaultPrinter.Text = CB_DefaultPrinter.Items(0)
        Else
            PrintLabel(Controllabel, "Ни один принтер не подключен!", 32, 564, Color.Red)
        End If
        GetCoordinats()
        PrintLabel(Controllabel, "", 32, 564, Color.Red)
#End Region
        '___________________________________________________________
        GB_UserData.Location = New Point(10, 12)
        L_UserName.Text = ""
        GB_UserData.Visible = True
        GB_WorkAria.Visible = False
    End Sub
#End Region
#Region "Регистрация пользователя"
    Dim UserInfo As New ArrayList()
    Private Sub TB_RFIDIn_KeyDown(sender As Object, e As KeyEventArgs) Handles TB_RFIDIn.KeyDown
        TB_RFIDIn.MaxLength = 10
        If e.KeyCode = Keys.Enter And TB_RFIDIn.TextLength = 10 Then ' если длина номера равна 10, то запускаем процесс
            UserInfo = GetUserData(TB_RFIDIn.Text, GB_UserData, GB_WorkAria, L_UserName, TB_RFIDIn)
            If UserInfo.Count <> 0 Then
                'TextBox3.Text = "UserID = " & UserInfo(0) & vbCrLf &
                '                "Name = " & UserInfo(1) & vbCrLf &
                '                "User Group = " & UserInfo(2) & vbCrLf  'UserInfo
                TB_RFIDIn.Clear()
            End If
        ElseIf e.KeyCode = Keys.Enter Then
            TB_RFIDIn.Clear()
        End If
    End Sub 'регистрация пользователя
#End Region
#Region "LabelPrint"
    Public Sub PrintLabel(Lab As Label, Message As String, x As Integer, y As Integer, Col As Color)
        Lab.Text = Message
        Lab.Location = New Point(x, y)
        Lab.Font = New Font("Microsoft Sans Serif", 40, FontStyle.Bold)
        Lab.ForeColor = Col
        Lab.Visible = True
    End Sub
#End Region
#Region "GetCoordinats"
    Private Sub GetCoordinats()
        Coordinats = New ArrayList
        Try
            For Each item In File.ReadAllLines("C:\Conract_LabelSet\Coordinats.csv")
                Coordinats.Add(item.Split(";")(0))
                Coordinats.Add(item.Split(";")(1))
            Next
            Num_X.Value = Coordinats(0)
            Num_Y.Value = Coordinats(1)
        Catch ex As Exception
            Dim PrinterInfo() As String = New String(0) {$"0;0"}
            IO.Directory.CreateDirectory("C:\Conract_LabelSet\")
            File.Create("C:\Conract_LabelSet\Coordinats.csv").Close()
            File.WriteAllLines("C:\Conract_LabelSet\Coordinats.csv", PrinterInfo)
            GetCoordinats()
        End Try
    End Sub
    Private Sub BT_Save_Coordinats_Click(sender As Object, e As EventArgs) Handles BT_Save_Coordinats.Click
        File.WriteAllText("C:\Conract_LabelSet\Coordinats.csv", $"{Num_X.Value};{Num_Y.Value}")
        GetCoordinats()
    End Sub
#End Region
#Region "Изменение числа этикеток еа печать"
    Private Sub Num_SNToPrint_ValueChanged(sender As Object, e As EventArgs) Handles Num_SNToPrint.ValueChanged
        PrintLabel(Controllabel, "", 32, 564, Color.Red)
        LoadGridFromDB(DG_SN_ForPrint, $"select tt.num,tt.sn,tt.IMEI,tt.IMEI2,tt.MAC_WF,tt.MAC_BT
            from(select top ({Num_SNToPrint.Value}) * , ROW_NUMBER() over(partition by isprinted order by id) num 
            FROM [FAS].[dbo].[CT_Aquarius]
            where LOTID = {LOTID} and IsPrinted = 0)tt")
    End Sub
#End Region
#Region "BT_Refrash"
    Private Sub BT_Refrash_Click(sender As Object, e As EventArgs) Handles BT_Refrash.Click
        Num_SNToPrint.Value = 0
        SN_InLOT_Ready = SelectInt($"select COUNT (*) from [FAS].[dbo].[CT_Aquarius]where LOTID = {LOTID} and  IsPrinted = 0")
        LoadGridFromDB(DG_SN_ForPrint, $"Select tt.num,tt.sn,tt.IMEI,tt.IMEI2,tt.MAC_BT,tt.MAC_WF
            from(select top ({Num_SNToPrint.Value}) * , ROW_NUMBER() over(partition by isprinted order by id) num 
            FROM [FAS].[dbo].[CT_Aquarius]
            where LOTID = {LOTID} and IsPrinted = 0)tt")
        If SN_InLOT_Ready > 0 Then
            LB_SN_InLOT_Ready.ForeColor = Color.Green
            LB_SN_InLOT_Ready.Text = SN_InLOT_Ready
        Else
            LB_COUNT_INLOT.ForeColor = Color.Red
            LB_SN_InLOT_Ready.Text = ""
            LB_COUNT_INLOT.Text = "Номера в лоте закончились"
            Num_SNToPrint.Enabled = False
        End If
    End Sub
#End Region
#Region "BT_GenSN"
    Private Sub BT_GenSN_Click(sender As Object, e As EventArgs) Handles BT_GenAndPrintSN.Click
        Dim setPrint As String = "", ContentToPrint As String = ""
        DG_Printed_SN.Rows.Clear()
        setPrint = "
SIZE 84.5 mm, 24 mm
GAP 2 mm, 0 mm
DIRECTION 0,0
REFERENCE 0,0
OFFSET 0 mm
SET PEEL OFF
SET CUTTER OFF
SET PARTIAL_CUTTER OFF
SET TEAR ON
CLS"
        If DG_SN_ForPrint.Rows.Count > 0 Then
            For i = 0 To DG_SN_ForPrint.Rows.Count - 1 Step 2
                DG_Printed_SN.Rows.Add(DG_SN_ForPrint.Item(0, i).Value, DG_SN_ForPrint.Item(1, i).Value)
                DG_Printed_SN.Rows.Add(DG_SN_ForPrint.Item(0, i + 1).Value, DG_SN_ForPrint.Item(1, i + 1).Value)
                ContentToPrint += GetContent(DG_SN_ForPrint.Item(1, i).Value, DG_SN_ForPrint.Item(1, i + 1).Value,
                                             DG_SN_ForPrint.Item(2, i).Value, DG_SN_ForPrint.Item(3, i).Value,
                                             DG_SN_ForPrint.Item(2, i + 1).Value, DG_SN_ForPrint.Item(3, i + 1).Value,
                                             DG_SN_ForPrint.Item(4, i).Value, DG_SN_ForPrint.Item(4, i + 1).Value,
                                             DG_SN_ForPrint.Item(5, i).Value, DG_SN_ForPrint.Item(5, i + 1).Value,
                                             Coordinats)
                RunCommand($"update  [FAS].[dbo].[CT_Aquarius] set [IsPrinted] = 1,[PrintByID] = {UserInfo(0)} , [PrintDate] = CURRENT_TIMESTAMP  where sn in ('{DG_SN_ForPrint.Item(1, i).Value}','{DG_SN_ForPrint.Item(1, i + 1).Value}' )")
                For j = i To i + 1
                    RunCommand($"if ( SELECT [ID] FROM [FAS].[dbo].[Ct_FASSN_reg] where SN = '{DG_SN_ForPrint.Item(1, j).Value}') is null
                                insert into [FAS].[dbo].[Ct_FASSN_reg] ([SN],[LOTID],[UserID],[AppID],[LineID],[RegDate])values
                                ('{DG_SN_ForPrint.Item(1, j).Value}',{LOTID},{UserInfo(0)},{IDApp},21,CURRENT_TIMESTAMP)")
                Next

            Next
            Print(setPrint + ContentToPrint)
            BT_Refrash_Click(sender, e)
            PrintLabel(Controllabel, "Номера отправлены на печать!", 20, 350, Color.Green)
        End If
    End Sub
#End Region
