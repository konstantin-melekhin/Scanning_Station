#Region "Возврат номеров в базу"
    Private Sub BT_SN_Reapload_Click(sender As Object, e As EventArgs) Handles BT_SN_Reapload.Click
        If DG_Printed_SN.Rows.Count > 0 Then
            Dim idx As Integer = DG_Printed_SN.CurrentCell.RowIndex
            Dim sn As String = DG_Printed_SN.Item(1, idx).Value
            Dim result As DialogResult = MessageBox.Show($"Вы возвращаете номер {sn}", "Вы уверены?", MessageBoxButtons.YesNo)
            If result = DialogResult.No Then
                MessageBox.Show($"Номер {sn} не возвращен в базу!")
            ElseIf result = DialogResult.Yes Then
                RunCommand($"update  [FAS].[dbo].[CT_Aquarius] set [IsPrinted] = 0  where sn ='{sn}'")
                MessageBox.Show($"Номер {sn} готов к повторной печати!")
                DG_Printed_SN.Rows.Remove(DG_Printed_SN.CurrentRow)
            End If
            BT_Refrash_Click(sender, e)
        End If
    End Sub
